    <script>
        var app=angular.module("MyApp",[]);
        app.controller("HomeController",function ($scope) {
            $scope.products=[
                {category:'MCA',subject:['C-Language','java','Python']},
                {category:'DCA',subject:['MS-Office','PhotoShop','Excel']}
            ];
        });
    </script>
    <style>
        fieldset{
            width: 200px;
            margin: 100px auto;
        }
    </style>
</head>
<body ng-app="MyApp" ng-controller="HomeController">
    <fieldset>
        <legend>Syllabus</legend>
        <ol>
            <li ng-repeat="item in products">
                {{item.category}}
                <ul>
                    <li ng-repeat="prod in item.subject">
                        {{prod}}
                    </li>
                </ul>
            </li>
        </ol>
    </fieldset>
